    "import math\n",
    "\n",
    "# Convert degrees to radians\n",
    "def degrees_to_radians(degrees):\n",
    "    return degrees * math.pi/180\n",
    "\n",
    "def radians_to_degrees(radians):\n",
    "    return radians * 180/math.pi\n",
    "\n",
    "degrees = 180\n",
    "radians = degrees_to_radians(180)\n",
    "print(radians)"
